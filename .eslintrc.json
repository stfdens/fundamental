{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "no-underscore-dangle": "off",
        "camelcase": "off",
        "consistent-return": "off",
        "operator-linebreak": "off",
        "max-len": "off"
    }
}
